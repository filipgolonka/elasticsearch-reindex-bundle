{
    "name": "filipgolonka/elasticsearch-reindex-bundle",
    "type": "library",
    "description": "Elasticsearch reindex bundle",
    "autoload": {
        "psr-4": {
            "": "src/",
            "spec\\": "spec/"
        }
    },
    "require": {
        "php": "~7.0",
        "elasticsearch/elasticsearch": "^2.0, <5.0",
        "symfony/console": "~3.0",
        "symfony/config": "~3.0",
        "symfony/http-kernel": "~3.0",
        "symfony/framework-bundle": "~3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.4",
        "behat/behat": "^3.3",
        "phing/phing": "^2.16",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "~2.5",
        "sebastian/phpcpd": "^3.0",
        "ralfmaxxx/phpmd_variable_lifetime": "^0.1.0",
        "ralfmaxxx/phpcs_rules": "0.2.0-alpha"
    },
    "config": {
        "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Filip Golonka",
            "email": "filip.golonka@schibsted.pl"
        }
    ]
}
